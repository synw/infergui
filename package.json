{
  "name": "infergui",
  "version": "0.1.2",
  "scripts": {
    "dev": "vite",
    "local": "vite preview --port 5173",
    "server": "node dist/bin/server.js",
    "build:compile": "vite build",
    "build:compress": "gzipper c ./dist --verbose --incremental --exclude avif,jpg --threshold 999 --remove-larger --brotli --brotli-param-mode font",
    "build:server": "tsc -p tsconfig_bin.json --outDir dist/bin/",
    "build": "run-s build:compile build:compress build:server"
  },
  "dependencies": {
    "@docdundee/vue": "^0.0.21",
    "@intrinsicai/gbnfgen": "^0.10.0",
    "@koa/cors": "^5.0.0",
    "@locallm/api": "^0.0.21",
    "@ow3/deep-unref-vue": "^0.1.3",
    "@snowind/header": "^0.0.11",
    "@snowind/plugin": "^0.5.0",
    "@snowind/state": "^0.1.0",
    "@snowind/switch": "^0.1.1",
    "@vueuse/core": "^10.2.1",
    "highlight.js": "^11.8.0",
    "koa": "^2.14.2",
    "koa-router": "^12.0.1",
    "koa-static": "^5.0.0",
    "llama-tokenizer-js": "^1.1.1",
    "localforage": "^1.10.0",
    "modprompt": "0.3.5",
    "primeicons": "^6.0.1",
    "primevue": "^3.39.0",
    "restmix": "^0.3.0",
    "vue": "^3.2.47",
    "vue-router": "4.1.6",
    "vuecodit": "^0.0.11"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.57",
    "@locallm/types": "^0.0.13",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.10",
    "@types/koa": "^2.13.12",
    "@types/koa-router": "^7.4.8",
    "@types/koa-static": "^4.0.4",
    "@types/node": "^20.4.8",
    "@vitejs/plugin-vue": "^4.2.1",
    "@vue/compiler-sfc": "^3.2.47",
    "autoprefixer": "^10.4.14",
    "gzipper": "^7.2.0",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "postcss": "^8.4.23",
    "sass": "^1.69.0",
    "tailwindcss": "^3.3.2",
    "tailwindcss-semantic-colors": "^0.2.0",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "unplugin-icons": "^0.15.3",
    "unplugin-vue-components": "^0.22.12",
    "vite": "^4.3.3"
  },
  "type": "module",
  "bin": {
    "infergui": "dist/bin/server.js"
  },
  "preferGlobal": true,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT"
}