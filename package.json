{
  "name": "infergui",
  "version": "0.6.1",
  "scripts": {
    "dev": "vite",
    "local": "vite preview --port 5173",
    "server": "node dist/bin/server.js",
    "devserver": "node  --loader ts-node/esm --watch bin/server.ts -d",
    "build:compile": "vite build",
    "build:compress": "gzipper c ./dist --verbose --incremental --exclude avif,jpg --threshold 999 --remove-larger --brotli --brotli-param-mode font",
    "build:server": "tsc -p tsconfig_bin.json --outDir dist/bin/",
    "build": "run-s build:compile build:compress build:server"
  },
  "dependencies": {
    "@agent-smith/tfm": "^0.0.1",
    "@agent-smith/tmem": "^0.0.3",
    "@docdundee/vue": "^0.0.21",
    "@intrinsicai/gbnfgen": "^0.12.0",
    "@koa/cors": "^5.0.0",
    "@locallm/api": "^0.0.30",
    "@ow3/deep-unref-vue": "^0.1.3",
    "@snowind/header": "^0.1.0",
    "@snowind/plugin": "^0.5.0",
    "@snowind/state": "^0.2.0",
    "@snowind/switch": "^0.1.1",
    "@vueuse/core": "^10.9.0",
    "highlight.js": "^11.9.0",
    "koa": "^2.15.3",
    "koa-bodyparser": "^4.4.1",
    "koa-router": "^12.0.1",
    "koa-static": "^5.0.0",
    "llama-tokenizer-js": "^1.2.1",
    "localforage": "^1.10.0",
    "modprompt": "0.7.2",
    "primeicons": "^7.0.0",
    "primevue": "^3.52.0",
    "restmix": "^0.4.2",
    "vue": "^3.4.26",
    "vue-router": "4.3.2",
    "vuecodit": "^0.0.11"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.205",
    "@locallm/types": "^0.0.16",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "@types/koa": "^2.15.0",
    "@types/koa-router": "^7.4.8",
    "@types/koa-static": "^4.0.4",
    "@types/node": "^20.12.7",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/compiler-sfc": "^3.4.26",
    "autoprefixer": "^10.4.19",
    "gzipper": "^7.2.0",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "postcss": "^8.4.38",
    "sass": "^1.76.0",
    "tailwindcss": "^3.4.3",
    "tailwindcss-semantic-colors": "^0.2.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "unplugin-icons": "^0.19.0",
    "unplugin-vue-components": "^0.27.0",
    "vite": "^5.2.10"
  },
  "type": "module",
  "bin": {
    "infergui": "dist/bin/server.js"
  },
  "preferGlobal": true,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT"
}